{
  "trans": {
    "put": "./src",
    "out": "./dist/trans",
    "process": {
      "svg": ["InkPng", null, "stream"]
    }
  },
  "white": {
    "put": "./src",
    "out": "./dist/white",
    "process": {
      "svg": ["InkPngWhite", null, "stream"]
    }
  },
  "pdf": {
    "put": "./src",
    "out": "./dist/pdf",
    "process": {
      "svg": ["InkPdf", null, "stream"]
    },
    "weirs": [
      ["./merge", {
        "svg": ["InkPdfMerge", null, "stream"]
      }],
      ["./printable/pamphlet", {
        "svg": ["InkPdfMerge", null, "stream"]
      }]
    ]
  },
  "print": {
    "put": "./src/printable",
    "out": "./dist/printed",
    "process": {
      "svg": ["InkPdfPrint", null, "stream"]
    },
    "weirs": [
      ["./pamphlet", {
        "svg": ["InkPdfMergePrint", null, "stream"]
      }]
    ]
  }
}
