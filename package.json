{
  "license": "MIT",
  "scripts": {
    "build": "chin -c -r babel-register",
    "png": "cross-env CHIN_ENV=PNG yarn build",
    "png:trans": "cross-env CHIN_ENV=PNG:TRANS yarn build",
    "png:all": "run-s png png:trans",
    "pdf": "cross-env CHIN_ENV=PDF yarn build",
    "print": "cross-env CHIN_ENV=PRINT yarn build",
    "pdf:all": "run-s pdf print",
    "both": "cross-env CHIN_ENV=BOTH yarn build",
    "both:pdf": "cross-env CHIN_ENV=BOTH:PDF yarn build",
    "both:all": "run-s both both:pdf",
    "all": "run-s png:all pdf:all both:all"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chin": "^3.0.2",
    "chin-plugin-inkscape": "^0.0.3",
    "cross-env": "^5.1.4",
    "npm-run-all": "^4.1.2"
